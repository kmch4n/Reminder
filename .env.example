# LINE Messaging API Credentials
# Get these from https://developers.line.biz/console/
LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token_here
LINE_CHANNEL_SECRET=your_channel_secret_here

# Public webhook URL (optional, for display purposes only)
REMINDER_PUBLIC_URL=https://your-domain.com/reminder/callback

# Data directory (optional, default: ./data)
REMINDER_DATA_DIR=./data

# Timezone (optional, default: Asia/Tokyo)
REMINDER_TIMEZONE=Asia/Tokyo
